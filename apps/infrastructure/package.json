{
  "name": "turbo-remote-cache-aws",
  "version": "0.1.0",
  "bin": {
    "turbo-remote-cache": "bin/turbo-remote-cache.js"
  },
  "scripts": {
    "build:lambda": "tsc -p lambda/tsconfig.json && esbuild lambda/src/**/*.ts --bundle --format=esm --platform=node --target=node20 --outdir=lambda/dist --out-extension:.js=.mjs --minify --external:@aws-sdk/client-s3",
    "build": "pnpm run build:lambda && cdk synth",
    "deploy": "pnpm run build && cdk deploy",
    "destroy": "cdk destroy",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "aws-cdk": "2.156.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "aws-cdk-lib": "2.156.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "esbuild": "^0.23.1",
    "source-map-support": "^0.5.21"
  }
}