{
  "name": "@turbo-remote-cache/construct",
  "version": "0.0.0",
  "private": true,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build:lambda": "tsc -p lambda/tsconfig.json && esbuild lambda/src/**/*.ts --bundle --format=esm --platform=node --target=node20 --outdir=lambda/dist --out-extension:.js=.mjs --minify --external:@aws-sdk/client-s3",
    "build": "pnpm run build:lambda && tsc",
    "test": "jest",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "aws-cdk-lib": "^2.156.0",
    "constructs": "^10.0.0",
    "typescript": "~5.5.3",
    "dotenv": "^16.4.5",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.156.0",
    "constructs": "^10.0.0"
  }
}