{
  "name": "turbo-remote-cache-construct",
  "version": "0.1.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "lambda/dist/**/*"
  ],
  "scripts": {
    "build:lambda": "tsc -p lambda/tsconfig.json && esbuild lambda/src/**/*.ts --bundle --format=esm --platform=node --target=node20 --outdir=lambda/dist --out-extension:.js=.mjs --minify --external:@aws-sdk/*",
    "build": "pnpm run build:lambda && tsc",
    "test": "jest",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.645.0",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@aws-sdk/util-dynamodb": "^3.645.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "@types/uuid": "^10.0.0",
    "aws-cdk-lib": "^2.156.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "typescript": "~5.5.3"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.156.0",
    "constructs": "^10.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}